generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  username  String   @unique
  password  String
  role      String
}

model Book {
  id        Int      @id @default(autoincrement())
  title     String
  isbn      String   @unique
  
}

